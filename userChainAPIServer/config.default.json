{
    "StratumServerCaseInsensitive": false,

    "ZKBroker": [
        "127.0.0.1:52181"
    ],
    "ZKSwitcherWatchDir": "/stratumSwitcher/bitcoin/",

    "UserListAPI": {
        "btc": "http://127.0.0.1:58080/userlist-btc.php",
        "bcc": "http://127.0.0.1:58080/userlist-bch.php"
    },
    "FetchUserListIntervalSeconds": 10,

    "EnableAPIServer": true,
    "ListenAddr": "0.0.0.0:8080",
    "APIUser": "admin",
    "APIPassword": "admin",
    "AvailableCoins": [
        "btc",
        "bcc"
    ],

    "FetchUserCoinIntervalSeconds": 60,
    "UserCoinMapURL": "http://127.0.0.1:58080/usercoin.php",

    "EnableUserAutoReg": true,
    "ZKAutoRegWatchDir": "/stratumSwitcher/bitcoin_autoreg/",
    "UserAutoRegAPI": {
        "IntervalSeconds": 10,
        "URL": "http://127.0.0.1:58080/autoreg.php",
        "User": "admin",
        "Password": "admin",
        "DefaultCoin": "btc",
        "PostData": {
            "sub_name": "{sub_name}",
            "region_name": "cn",
            "currency": "BTC"
        }
    },

    "ZKSubPoolUpdateBaseDir": "/subpool/",
    "ZKSubPoolUpdateAckTimeout": 5
}